package templates

templ Profile(data string) {
  <!DOCTYPE html>
  <html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>PROFILE VIEW</title>
  </head>
  <body>
    <h1>Profile</h1>
    <div>
      { data }
    </div>
    <a href="/auth/logout">Logout</a>
  </body>
  </html>
  
}